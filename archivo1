import java.io.File;
import java.io.IOException;

public class EjemploFile01 {

	public static void main(String[] args)  {
		File f = new File("XXXXX111.txt");
		File f1 = new File("XXX111.txt");
		try {
			f.createNewFile();
			f1.createNewFile();
		} catch (IOException e) {
			
			e.printStackTrace();
		} finally {
		System.out.println("Fin del programa");
		}
	}
}
